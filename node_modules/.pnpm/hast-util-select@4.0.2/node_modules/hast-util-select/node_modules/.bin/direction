#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/direction@1.0.4/node_modules/direction/cli.js/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/direction@1.0.4/node_modules/direction/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/direction@1.0.4/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/node_modules:/Users/kevinabergkultalahti/Workspace/node_modules:/Users/kevinabergkultalahti/node_modules:/Users/node_modules:/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/hast-util-select@4.0.2/node_modules/hast-util-select/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/hast-util-select@4.0.2/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../direction@1.0.4/node_modules/direction/cli.js" "$@"
fi
