#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/nollup@0.15.3/node_modules/nollup/lib/cli.js/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/nollup@0.15.3/node_modules/nollup/lib/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/nollup@0.15.3/node_modules/nollup/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/nollup@0.15.3/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/node_modules:/Users/kevinabergkultalahti/Workspace/node_modules:/Users/kevinabergkultalahti/node_modules:/Users/node_modules:/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/rollup-plugin-svelte-hot@0.11.2_svelte@3.35.0/node_modules/rollup-plugin-svelte-hot/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/rollup-plugin-svelte-hot@0.11.2_svelte@3.35.0/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nollup@0.15.3/node_modules/nollup/lib/cli.js" "$@"
else 
  exec node  "$basedir/../../../../../nollup@0.15.3/node_modules/nollup/lib/cli.js" "$@"
fi
