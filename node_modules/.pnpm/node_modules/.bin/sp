#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/snowpack@3.0.13/node_modules/snowpack/index.bin.js/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/snowpack@3.0.13/node_modules/snowpack/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/snowpack@3.0.13/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/node_modules:/Users/kevinabergkultalahti/Workspace/node_modules:/Users/kevinabergkultalahti/node_modules:/Users/node_modules:/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/node_modules/snowpack/index.bin.js/node_modules:/Users/kevinabergkultalahti/Workspace/Contracting/Budibase/Colorpicker/node_modules/.pnpm/node_modules/snowpack/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../snowpack/index.bin.js" "$@"
else 
  exec node  "$basedir/../snowpack/index.bin.js" "$@"
fi
